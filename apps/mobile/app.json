{
  "expo": {
    "name": "Tradie App",
    "slug": "tradie-app",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#2563eb"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tradieapp"
    },
    "android": {
      "package": "com.tradieapp"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "scheme": "tradieapp",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
